{
  "name": "react-javascript-starter",
  "version": "1.0.0",
  "description": "A starting point for a web app built with React/JavaScript.",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand --coverage",
    "lint": "eslint . --ext .jsx",
    "start:dev": "npm run build:dev && webpack-dev-server --open --mode development --inline --hot",
    "build:dev": "webpack -d",
    "build": "webpack -p && npm run build:css",
    "build:css": "postcss public/assets/styles/tailwind.css -o public/assets/styles/tailwind.build.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thegenesisproject/react-javascript-starter.git"
  },
  "keywords": [
    "react",
    "javascript",
    "es6",
    "webpack",
    "web",
    "boilerplate",
    "baseline"
  ],
  "author": "Robin Hood <robinhood.experiment@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thegenesisproject/react-javascript-starter/issues"
  },
  "homepage": "https://github.com/thegenesisproject/react-javascript-starter#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "autoprefixer": "^9.8.6",
    "css-loader": "^4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "html-loader": "^1.2.1",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^0.11.2",
    "postcss-cli": "^7.1.2",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "tailwindcss": "^1.8.8",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
